{
  "name": "lazy-array",
  "version": "0.0.0",
  "description": "Lazy evaluation array utilities",
  "license": "MIT",
  "author": "Hamza Arif",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/hamza-arif-dev/lazy-array"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --dts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write . --ignore-path .gitignore"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.3",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3"
  }
}
